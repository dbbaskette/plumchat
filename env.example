# OpenAI Configuration (required for AI responses)
OPENAI_API_KEY=your-openai-api-key-here

# MCP Server URLs (optional - defaults shown)
MCP_SCHEMA_SERVER_URL=http://localhost:8080
MCP_QUERY_SERVER_URL=http://localhost:8081
MCP_MGMT_SERVER_URL=http://localhost:8082

